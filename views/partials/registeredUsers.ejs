<div class="card">
    <h5 class="card-header"> Registered Users</h5>
    <div class="card-body">
        <ul class="list-group">
            <% if (typeof users != 'undefined') {%>
            <% users.forEach(function(username){ %>
            <li class="list-group-item"> <%= username %></li>
            <% })} %>
        </ul>
    </div>
</div>